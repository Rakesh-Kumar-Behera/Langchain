{
    "title": "student",
    "description": "A schema representing a student",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the student"
        },
        "age": {
            "type": "integer",
            "description": "The age of the student"
        },
        "courses": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "description": "List of courses the student is enrolled in"
        }
    },
    "required": ["name", "age"]
}